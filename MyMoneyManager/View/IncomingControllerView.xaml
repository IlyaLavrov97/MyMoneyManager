<UserControl x:Class="MyMoneyManager.View.IncomingControllerView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:MyMoneyManager.View"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewmodels="clr-namespace:MyMoneyManager.ViewModel"
             d:DesignHeight="300"
             d:DesignWidth="300"
             mc:Ignorable="d">

    <UserControl.DataContext>
        <viewmodels:IncomingControllerViewModel />
    </UserControl.DataContext>

    <Grid>
        <ComboBox Grid.Row="1"
                  Width="180"
                  Height="28"
                  Margin="14,192,10,0">
            <ComboBoxItem Content="Normal" />
            <ComboBoxItem Content="Яблоко" />
            <ComboBoxItem Content="Арбуз" />
            <ComboBoxItem Content="Апельсин" />
            <ComboBoxItem Content="Гранат" />
            <ComboBoxItem Content="Банан" />
        </ComboBox>
    </Grid>
</UserControl>
